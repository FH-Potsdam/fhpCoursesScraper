{
  "name": "fhp-courses-scraper",
  "version": "1.0.0",
  "description": "This scrapper is designed to extract informations about the courses given in the University of Applied Science Potsdam. More precisely, it focuses on scrapping the online course catalog of the school (https://www.fh-potsdam.de/studieren/design/studium/vorlesungsverzeichnis/).",
  "main": "index.js",
  "scripts": {
    "start": "webpack --env.prod -p && node ./dist/courses.js & node ./dist/people.js",
    "dev": "webpack --env.dev && node ./dist/courses.js & node ./dist/people.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FH-Potsdam/fhpCoursesScraper.git"
  },
  "dependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "jquery": "^3.1.1",
    "moment": "^2.15.1",
    "moment-timezone": "^0.5.6",
    "nightmare": "^2.7.0",
    "vo": "^4.0.2",
    "webpack": "^2.1.0-beta.25"
  },
  "keywords": [
    "scraper",
    "fhp",
    "courses",
    "potsdam",
    "nightmare",
    "headless",
    "rawdataprepare"
  ],
  "author": "Jonas KÃ¶pfer, Joseph Ribbe",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/FH-Potsdam/fhpCoursesScraper/issues"
  },
  "homepage": "https://github.com/FH-Potsdam/fhpCoursesScraper#readme",
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "eslint": "^3.8.0"
  }
}
